<h1>Created Events</h1>

<% @user.created_events.each do |event| %>
  <h3>*<%= event.name %></h3>
<% end %>


<br /><br /><br />


<h2>Past Attended Events</h2>

<% @past_events = @user.attended_events.where('event_date < ?', Time.now) %>
<% @future_events = @user.attended_events.where('event_date > ?', Time.now) %>
<% @past_events.each do |event| %>
  <p><%= event.name %></p><br />
<% end %>

<br /><br /><br />

<h2>Future Attended Events</h2>

<% @future_events.each do |event| %>
  <p><%= event.name %></p><br />
<% end %>
